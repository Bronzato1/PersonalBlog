@model List<PersonalBlog.Models.CustomUser>

Manage users and roles

<table class="table table-borderless table-striped table-underlined">
    <thead>
        <th>Username</th>
        <th>Firstname</th>
        <th>Lastname</th>
        <th>Roles</th>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.UserName</td>
                <td>@user.FirstName</td>
                <td>@user.LastName</td>
                <td></td>
            </tr>
        }
    </tbody>
</table>
